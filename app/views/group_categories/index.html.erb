<h1>Listing group_categories</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>

<% @group_categories.each do |group_category| %>
  <tr>
    <td><%=h group_category.name %></td>
    <td><%= link_to 'Show', group_category %></td>
    <td><%= link_to 'Edit', edit_group_category_path(group_category) %></td>
    <td><%= link_to 'Destroy', group_category, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New group_category', new_group_category_path %>